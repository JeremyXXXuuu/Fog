{
    "cli": {
        "version": ">= 7.8.2"
    },
    "build": {
        "base": {
            "node": "18.17.1",
            "yarn": "4.1.1",
            "env": {
                "EXAMPLE_ENV": "example value"
            },
            "android": {
                "image": "default",
                "env": {
                    "PLATFORM": "android"
                }
            },
            "ios": {
                "image": "latest",
                "env": {
                    "PLATFORM": "ios"
                }
            }
        },
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {}
    },
    "submit": {
        "production": {}
    }
}